package ch10_static_final;

class Test {
	private int num;
	// 오버라이딩 : 부모클래스를 상속받아 부모클래스의 메소드를 재정의하는 것
	public final void print() {
		System.out.println(num); // 오버라이딩이 안되는 메소드
	}
}

public class FinalMethod extends Test{
	public void print(){ 
		System.out.println("자식 클래스의 print method");
	}
	
	public static void main(String[] args) {
	}
}
