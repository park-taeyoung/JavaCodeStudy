package ch10_static_final_polymorphism;

class AA {
	int a= 10;
	public void print() {
		System.out.println(a);
	}
}

class BB extends AA {
	int a= 20;
	public void print() {
		System.out.println(a);
	}
}


public class Poly {
	public static void main(String[] args) {
		BB b = new BB();
		b.print();
		AA aa = new BB();
		System.out.println(aa.a);
		aa.print();
	}
}
