package ch16_thread.ThreadJoin;

public class ThreadJoin extends Thread {
	public void run() {
		System.out.println(Thread.currentThread().getName()
				+"스레드 시작...");
		System.out.println(Thread.currentThread().getName()
				+"스레드 종료...");		
	}
	
	public static void main(String[] args) {
		System.out.println(Thread.currentThread().getName()
				+"스레드 시작...");
		System.out.println(Thread.currentThread().getName()
				+"스레드 종료...");		
	}
}
